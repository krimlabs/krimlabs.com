---
import BaseLayout from '@src/layouts/Base.astro';
import IndexContainer from '@src/containers/Index';
import { getTimeline } from '@src/domain/content';

const posts = await Astro.glob("/content/posts/*.md")
const timelineItems = getTimeline(posts);
---

<BaseLayout title={'Krim Labs'}>
  <IndexContainer timelineItems={timelineItems} />
</BaseLayout>
