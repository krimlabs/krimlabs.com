{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "bun-types",
      "@types/react",
      "@types/react-dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@contentlayer/generated": [
        ".contentlayer/generated"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "include": [
      "./contentlayer/generated"
    ]
  }
}
